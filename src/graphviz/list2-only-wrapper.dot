digraph {
    l1[class="fix"]
    l2[class="fix"]
    l3[class="fix"]
    l4[class="fix"]
    p1[label=3 class="cell,disabled"]
    p2[label=2 class="cell,disabled"]
    p3[label=1 class="cell,disabled"]
    nil[class="nil,disabled"]

    l1 -> p1[label=value]
    p1 -> l2[label=tail class="disabled"]
    l2 -> p2[label=value]
    p2 -> l3[label=tail class="disabled"]
    l3 -> p3[label=value]
    p3 -> l4[label=tail class="disabled"]
    l4 -> nil[label=value]
}
